# Open Balena API Configuration
# Copy this file to .env.local and update with your values

# Your Open Balena instance API URL
# For Open Balena: https://your-open-balena-instance.com
# For BalenaCloud: https://api.balena-cloud.com
NEXT_PUBLIC_BALENA_API_URL=https://api.balena-cloud.com

# API Version (usually v7)
BALENA_API_VERSION=v7

# Optional: Supervisor API timeout (in milliseconds)
# Default: 30000 (30 seconds)
BALENA_API_TIMEOUT=30000
